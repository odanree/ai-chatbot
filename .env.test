# Test Environment Variables
# These are used by CI/CD pipeline tests

# Use mock services for tests
MOCK_OPENAI=true
MOCK_SHOPIFY=true

# Dummy API keys for testing (not used due to mocking)
OPENAI_API_KEY=sk-test-key-12345678901234567890
SHOPIFY_STORE_DOMAIN=test-store.myshopify.com
SHOPIFY_STOREFRONT_ACCESS_TOKEN=test-storefront-token
SHOPIFY_ADMIN_API_TOKEN=test-admin-token

# Server config
PORT=3000
NODE_ENV=test
LOG_LEVEL=error

# Performance
REQUEST_TIMEOUT_MS=30000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000
